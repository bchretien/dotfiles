# This tmux statusbar config was created by tmuxline.vim
# on Fri, 15 Aug 2014

tmux set status-bg "colour88"
tmux set message-command-fg "colour232"
tmux set status-justify "left"
tmux set status-left-length "100"
tmux set status "on"
tmux set pane-active-border-fg "colour88"
tmux set message-bg "colour244"
tmux set status-right-length "100"
tmux set status-right-attr "none"
tmux set message-fg "colour232"
tmux set message-command-bg "colour244"
tmux set status-attr "none"
tmux set status-utf8 "on"
tmux set pane-border-fg "colour244"
tmux set status-left-attr "none"
tmux setw window-status-fg "colour253"
tmux setw window-status-attr "bold"
tmux setw window-status-activity-bg "colour88"
tmux setw window-status-activity-attr "none"
tmux setw window-status-activity-fg "colour253"
tmux setw window-status-separator ""
tmux setw window-status-bg "colour88"
tmux set status-left "#[fg=colour253,bg=colour88,bold] #S "
tmux setw window-status-format "#[fg=colour253,bg=colour88,bold] #I #[fg=colour253,bg=colour88,bold] #W "
tmux setw window-status-current-format "#[fg=colour88,bg=colour178,nobold,nounderscore,noitalics]#[fg=colour232,bg=colour178] #I #[fg=colour232,bg=colour178] #W #[fg=colour178,bg=colour88,nobold,nounderscore,noitalics]"
tmux set status-right "#[fg=colour88,bg=colour88,nobold,nounderscore,noitalics]#[fg=colour253,bg=colour88,bold] %d/%m/%Y  %H:%M #[fg=colour88,bg=colour88,nobold,nounderscore,noitalics]#[fg=colour253,bg=colour88,bold] #h "
